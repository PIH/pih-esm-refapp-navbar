{
  "name": "@pih/esm-refapp-navbar-app",
  "version": "1.0.5",
  "description": "A RefApp-styled navbar compatible with the standard styleguide",
  "browser": "dist/pih-esm-refapp-navbar-app.js",
  "main": "src/index.ts",
  "license": "MPL-2.0",
  "homepage": "https://pihemr.atlassian.net/wiki/spaces/DSS/pages/538116104/PIH+EMR+Referrals+Queue",
  "scripts": {
    "start": "openmrs develop --backend https://humci.pih-emr.org/ --api-url /mirebalais/",
    "serve": "webpack serve --mode development",
    "build": "webpack --mode production",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "eslint src --ext tsx",
    "prettier": "prettier 'src/**/*' --write",
    "typescript": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "update-translations": "tx pull"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "keywords": [
    "openmrs",
    "single-spa"
  ],
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently 'npm:lint' 'npm:test' 'npm:typescript' 'npm:update-translations' && git add translations/*.json"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pih/pih-esm-refapp-navbar.git"
  },
  "bugs": {
    "url": "https://github.com/pih/esm-refapp-navbar/issues"
  },
  "peerDependencies": {
    "@openmrs/esm-framework": "3.x",
    "react": "16.x",
    "react-dom": "16.x",
    "react-i18next": "^11.x"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@openmrs/esm-framework": "next",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.23",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/systemjs": "^6.1.0",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-i18next-extract": "^0.6.1",
    "concurrently": "^5.1.0",
    "coveralls": "^3.0.7",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "i18next": "^19.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lint-staged": "^9.4.3",
    "openmrs": "next",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "ramda": "^0.27.0",
    "react-i18next": "^11.3.1",
    "rxjs": "^6.5.4",
    "typescript": "^4.2.4"
  }
}
