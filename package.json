{
  "name": "@pih/esm-refapp-navbar-app",
  "version": "2.0.0",
  "description": "A RefApp-styled navbar compatible with the standard styleguide",
  "browser": "dist/pih-esm-refapp-navbar-app.js",
  "license": "MPL-2.0",
  "main": "src/index.ts",
  "source": true,
  "homepage": "https://pihemr.atlassian.net/wiki/spaces/DSS/pages/538116104/PIH+EMR+Referrals+Queue",
  "scripts": {
    "start": "openmrs develop",
    "serve": "webpack serve --mode=development",
    "build": "webpack --mode production",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typescript": "tsc",
    "test": "jest --config jest.config.json",
    "verify": "concurrently 'yarn:lint' 'yarn:test' 'yarn:typescript'",
    "coverage": "yarn test -- --coverage",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn verify"
    }
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "keywords": [
    "openmrs",
    "microfrontends"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pih/pih-esm-refapp-navbar.git"
  },
  "bugs": {
    "url": "https://github.com/pih/esm-refapp-navbar/issues"
  },
  "peerDependencies": {
    "@openmrs/esm-framework": "next",
    "react": "18.x",
    "react-i18next": "11.x",
    "react-router-dom": "6.x",
    "rxjs": "6.x"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@openmrs/esm-framework": "next",
    "@swc/core": "^1.2.203",
    "@swc/jest": "^0.2.21",
    "@testing-library/dom": "^7.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.4",
    "@types/react": "^18.0.14",
    "@types/react-dates": "^21.8.0",
    "@types/react-dom": "^18.0.5",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.5",
    "babel-plugin-i18next-extract": "^0.8.3",
    "concurrently": "^6.3.0",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "eslint-config-xo": "^0.39.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "i18next": "^21.2.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.5",
    "jest-cli": "^27.2.5",
    "lint-staged": "^11.2.0",
    "openmrs": "next",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "ramda": "^0.27.1",
    "react": "^18.2.0",
    "react-i18next": "^11.3.1",
    "rxjs": "^6.6.7",
